{
  "name": "CatchPhish",
  "description": "A Google Chrome extension that check for phishing website automatically",
  "version": "1.0",
  "manifest_version": 2,

  "permissions": ["activeTab", "tabs", "<all_urls>"],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png"
  }
}